<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyCheckbox" parent="@style/Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:buttonTint">#64B5F6</item>
    </style>

    <style name="HabitDialog" parent="@android:style/Theme.Dialog">
        <item name="android:background">@color/darkThemeBackground</item>
        <item name="android:textColor">@color/darkThemeTitle</item>
    </style>

</resources>
